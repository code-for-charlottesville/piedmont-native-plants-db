{
  "name": "piedmont-database",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen_react_js": "webpack --mode development",
    "gen_api_schema": "python manage.py spectacular --file openapi_files/schema.yml",
    "gen_api_client": "openapi-generator-cli generate -i openapi_files/schema.yml -g typescript-fetch -o ./openapi_files/database_api_client/ -c ./openapi_files/config.yml",
    "del_api_client": "python openapi_files/clean.py",
    "link_api_client": "cd openapi_files/database_api_client && npm link && cd .. && cd .. && npm link piedmont-api-client",
    "unlink_api_client": "npm unlink piedmont-api-client",
    "creact":  "npm run gen_react_js",
    "rebuild": "npm run del_api_client && npm run unlink_api_client && npm run gen_api_schema && npm run gen_api_client && npm run link_api_client && npm run gen_react_js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  }
}
